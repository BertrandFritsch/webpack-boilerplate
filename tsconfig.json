{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react",
    "moduleResolution": "node",
    "strict": true,
    "noEmit": true,
    "isolatedModules": false,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "preserveConstEnums": true,
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  }
}
